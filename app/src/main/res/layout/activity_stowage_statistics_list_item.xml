<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/stowage_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="?android:attr/selectableItemBackground"
        android:gravity="center"
        android:orientation="horizontal">

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.5"
            android:orientation="vertical">


            <TextView
                android:id="@+id/Operate_DateTime"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_horizontal"
                android:text="05-07"
                android:textColor="@color/color_ffffff"
                android:textSize="15sp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:paddingLeft="@dimen/dimen_5">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/weiqianshou"
                    android:layout_width="60dp"
                    android:layout_height="25dp"
                    android:layout_gravity="center_vertical|right"
                    android:layout_marginRight="100dp"
                    android:layout_marginTop="@dimen/dimen_13"
                    android:background="@drawable/un_finish"
                    android:gravity="center"
                    android:text="未到车"
                    android:textColor="#ffffff"
                    android:textSize="12sp"
                    android:visibility="gone" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:gravity="center"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="25dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/Traffic_Mode_btn"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/shape_rounded_btn_8ddc8c"
                                android:paddingLeft="@dimen/dimen_2"
                                android:paddingRight="@dimen/dimen_2"
                                android:text="整"
                                android:textColor="#ffffff"
                                android:textSize="@dimen/font_12" />

                            <TextView
                                android:id="@+id/CarCode_text"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/dimen_5"
                                android:gravity="center_vertical"
                                android:text="--"
                                android:textColor="#ffffff"
                                android:textSize="@dimen/font_14" />
                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="25dp"
                            android:layout_weight="1"
                            android:gravity="center_vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/stowage_list_item_gua"
                                android:textColor="#ffffff"
                                android:textSize="@dimen/font_14" />

                            <TextView
                                android:id="@+id/BoxCarCode_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dimen_5"
                                android:text="--"
                                android:textColor="#ffffff"
                                android:textSize="@dimen/font_14" />


                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:gravity="center_vertical">

                        <TextView
                            android:id="@+id/Line_Oid_btn"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textColor="#ffffff"
                            android:textSize="@dimen/font_14" />

                        <TextView
                            android:id="@+id/Line_Name_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dimen_5"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="--"
                            android:textColor="#ffffff" />

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="25dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/shape_rounded_tv_f5bb4e_3"
                                android:gravity="center_vertical"
                                android:paddingLeft="@dimen/dimen_2"
                                android:paddingRight="@dimen/dimen_2"
                                android:text="发"
                                android:textColor="#FFFFFF"
                                android:textSize="12sp" />

                            <TextView
                                android:id="@+id/sendCity_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="5dp"
                                android:gravity="center_vertical"
                                android:text="--"
                                android:textColor="@color/color_ffffff"
                                android:textSize="12sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/end_time_text_layout"
                            android:layout_width="0dp"
                            android:layout_height="25dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/dao_state_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/shape_rounded_tv_8dc15a_3"
                                android:gravity="center"
                                android:paddingLeft="@dimen/dimen_2"
                                android:paddingRight="@dimen/dimen_2"
                                android:text="到"
                                android:textColor="#FFFFFF"
                                android:textSize="12sp" />

                            <TextView
                                android:id="@+id/arriveCity_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="5dp"
                                android:gravity="center_vertical"
                                android:text="--"
                                android:textColor="@color/color_ffffff"
                                android:textSize="12sp" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/textView18"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/stowage_list_item_size"
                                android:textColor="#ffffff"
                                android:textSize="@dimen/font_14" />

                            <TextView
                                android:id="@+id/Ticket_Count_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dimen_5"
                                android:text="--"
                                android:textColor="#ffffff"
                                android:textSize="@dimen/font_14" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal"
                            android:visibility="invisible">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/stowage_list_item_profit"
                                android:textColor="#818181"
                                android:textSize="@dimen/font_14" />

                            <TextView
                                android:id="@+id/Profit_Fee_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dimen_5"
                                android:text="--"
                                android:textColor="#818181"
                                android:textSize="@dimen/font_14" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/foreView"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_0.5"
            android:layout_weight="4"
            android:background="@color/color_list_view_line"
            android:visibility="visible"></TextView>

        <TextView
            android:id="@+id/back_view"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_0.5"
            android:layout_weight="1"
            android:background="@color/color_list_view_line"></TextView>

    </LinearLayout>


</LinearLayout>